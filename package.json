{
  "name": "kubra-react-boilerplate",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "class-autobind": "^0.1.4",
    "classnames": "^2.2.5",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "4.0.2",
    "css-loader": "0.28.7",
    "detect-port": "1.0.1",
    "dotenv": "4.0.0",
    "draftjs-to-html": "^0.7.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "4.10.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "filesize": "3.3.0",
    "fs-extra": "3.0.1",
    "gzip-size": "3.0.0",
    "html-webpack-plugin": "2.29.0",
    "http-proxy-middleware": "0.17.2",
    "jest": "20.0.4",
    "jest-cli": "^20.0.4",
    "json-loader": "0.5.4",
    "lodash": "^4.17.4",
    "node-sass": "^4.7.2",
    "object-assign": "4.1.1",
    "path-exists": "2.1.0",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react-dev-utils": "^4.2.1",
    "recursive-readdir": "2.1.0",
    "sass-loader": "^4.1.1",
    "seamless-immutable": "^7.1.2",
    "strip-ansi": "3.0.1",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "dependencies": {
    "axios": "*",
    "babel-polyfill": "^6.22.0",
    "bluebird": "^3.5.0",
    "es5-shim": "^4.5.9",
    "formik": "^0.11.11",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "normalizr": "*",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "*",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.0",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-test-renderer": "^16.2.0",
    "redux": "*",
    "redux-logger": "*",
    "redux-thunk": "*",
    "yup": "^0.24.1"
  },
  "scripts": {
    "start": "NODE_PATH=src/ node scripts/start.js",
    "lint": "eslint src/",
    "start-windows": "cmd /V /C \"set NODE_PATH=src/ && node scripts/start.js\"",
    "build": "NODE_PATH=src/ node scripts/build.js",
    "build-windows": "cmd /V /C \"set NODE_PATH=src/ && node scripts/build.js\"",
    "travis": "npm run lint",
    "test": "NODE_PATH=src/ node scripts/test.js --env=jsdom",
    "build:symlinked": "cross-env NODE_PATH=./node_modules browserify src/index.js > dist/build.js",
    "deploy-dev": "REACT_APP_ENV=development NODE_PATH=src/ node scripts/build.js && aws s3 sync build/ s3://<bucket>",
    "deploy-dev-windows": "cmd /V /C \"set REACT_APP_ENV=development && set NODE_PATH=src/ && node scripts/build.js && aws s3 sync build/ s3://<bucket>\""
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.test.js?(x)",
      "<rootDir>/src/**/?(*+)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/src/config/*"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
